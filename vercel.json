{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/debug.py",
      "use": "@vercel/python"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9",
      "NPM_CONFIG_PYTHON": "/usr/local/bin/python",
      "LD_LIBRARY_PATH": "/usr/local/lib"
    },
    "use": "@vercel/python@latest",
    "installCommand": "chmod +x vercel_build.sh && ./vercel_build.sh"
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/debug/moviepy",
      "dest": "/api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9.18",
    "PIP_CONSTRAINT": "constraints.txt"
  },
  "buildCommand": "./build.sh",
  "functions": {
    "api/app.py": {
      "includeFiles": "**"
    },
    "api/health.py": {
      "includeFiles": "**"
    }
  }
}