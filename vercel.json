{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9.18",
    "PIP_CONSTRAINT": "constraints.txt"
  },
  "buildCommand": "./build.sh",
  "functions": {
    "api/app.py": {
      "includeFiles": "**"
    }
  }
}